/*
    求第n项斐波那契数

    1, 1, 2, 3, 5, 8, 13 ...

    第1项为1,
    第2项为1,
    第3项为2,
    第4项为3,
    第5项为5,
    第6项为8,
    第7项为13,
    ...
*/
// 绑定Fib函数
(
	Bind
	[Fib]
	[n -> {
			if
			// n < 3?
			[<
				[n]
				[3]
			]
			// 递归是否结束？
			[1]
			[+
				// Fib(n - 1)
				[Fib
					[-
						[n]
						[1]
					]
				]
				// Fib(n - 2)
				[Fib
					[-
						[n]
						[2]
					]
				]
			]
		}
	]
)
// 打印求得的 斐波那契数
(
	PrintNumber
	[Fib
		[7]
	]
)
